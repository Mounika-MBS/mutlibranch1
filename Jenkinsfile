pipeline {
    agent any

    stages {
        stage('development branch') {
            steps {
                sh 'echo "this is development branch"'
            }
        }

        stage('Sprint1 Application') {
            steps {
                sh 'echo "sprint1 application"'
            }
        }

        stage('deploy app') {
            steps {
                sh 'echo "deploying application"'
            }
        }
    }
}

